<h1> <%= @house.name %> </h1>
<h2> <%= link_to "Edit House", edit_house_path %> 
<%= link_to "Add Character", new_character_path %> </h2>
</br>
<%= image_tag @house.img_url %>

<% @house.characters.each do |char| %>
    <div>
        <h2>
            <%= link_to char.name, char %>
        </h2>
        <%= image_tag char.img_url%>
    </div>
<% end %>
